{
  "manifest_version": 2,
  "name": "Chrome Save",
  "version": "1.0",
  "description": "console.save() in chrome devtools",
  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["script.js"]
}
